{
    "name": "fluidtypo3/vhs",
    "description": "The vhs package from FluidTYPO3",
    "type": "typo3-cms-extension",
    "support": {
        "irc": "irc://irc.freenode.org/fedext",
        "issues": "https://github.com/FluidTYPO3/vhs/issues"
    },
    "keywords": [
        "TYPO3 CMS",
        "templating",
        "Utility",
        "ViewHelper",
        "fedext",
        "fluid"
    ],
    "homepage": "http://fluidtypo3.org",
    "license": "GPL-2.0+",
    "version": "2.1.4",
    "autoload": {
        "psr-4": {
            "FluidTYPO3\\Vhs\\": "Classes/"
        },
        "classmap": [
            "Tests/",
            "vendor/typo3/cms/typo3/sysext"
        ]
    },
    "replace": {
        "typo3/cms-composer-installers": "*"
    },
    "require-dev": {
        "satooshi/php-coveralls": "dev-master",
        "typo3/cms": "6.2.*",
        "phpunit/phpunit": "4.2.*",
        "mikey179/vfsStream": "~1.3.0"
    }
}
